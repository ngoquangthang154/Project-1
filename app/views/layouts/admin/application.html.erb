<!DOCTYPE html>
<html>
<head>
  <title><%= yield(:title) %></title>
    <%= render "layouts/admin/layouts/rails_default" %>
    <%= render "layouts/shim" %>
</head>
<body>
  <div class="wrapper">
    <%= render "layouts/admin/layouts/sidebar" %>
    <div class="content content-web">
      <%= render "layouts/admin/layouts/header" %>
          <% flash.each do |message_type, message| %>
          <%= content_tag :div, message, class: "alert alert-#{message_type}" %>
          <% end %>
          <%= yield %>
    </div>
  </div>
</body>
</html>
